{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generated schema for Root",
  "type": "array",
  "properties": {
    "id": {
      "type": "integer",
      "multipleOf": 1,
      "minimum": 1
    },
    "username": {
      "type": "string",
      "minLength": 4,
      "maxLength": 25
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "fullName": {
      "type": "string",
      "minLength": 0,
      "maxLength": 100
    },
    "phoneNumber": {
      "type": "string",
      "pattern": "^\\+?[1-9]\\d{1,14}$"
    },
    "createdAt": {
      "type": "string"
    },
    "updatedAt": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "username",
    "email",
    "fullName"
  ]
}